FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 6.71513471484294e-07;

boundaryField
{
    Inlet
    {
      type             inletOutlet;
      inletValue         $internalField;
    }

    Outlet
    {
      type              zeroGradient;
    }


    Side
    {
        type            empty;
    }
   
    Airfoil
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    Ground
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}

